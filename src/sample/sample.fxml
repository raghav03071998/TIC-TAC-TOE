<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button fx:id="zero" GridPane.rowIndex="0" GridPane.columnIndex="0" minWidth="25" onAction="#buttonpressed" ></Button>
    <Button fx:id="one" GridPane.rowIndex="0" GridPane.columnIndex="1" minWidth="25" onAction="#buttonpressed"></Button>
    <Button fx:id="two" GridPane.rowIndex="0" GridPane.columnIndex="2" minWidth="25" onAction="#buttonpressed" ></Button>
    <Button fx:id="three" GridPane.rowIndex="1" GridPane.columnIndex="0" minWidth="25" onAction="#buttonpressed"></Button>
    <Button fx:id="four" GridPane.rowIndex="1" GridPane.columnIndex="1" minWidth="25" onAction="#buttonpressed"></Button>
    <Button fx:id="five" GridPane.rowIndex="1" GridPane.columnIndex="2" minWidth="25" onAction="#buttonpressed"></Button>
    <Button fx:id="six" GridPane.rowIndex="2" GridPane.columnIndex="0" minWidth="25" onAction="#buttonpressed"></Button>
    <Button fx:id="seven" GridPane.rowIndex="2" GridPane.columnIndex="1" minWidth="25" onAction="#buttonpressed"></Button>
    <Button fx:id="eight" GridPane.rowIndex="2" GridPane.columnIndex="2" minWidth="25" onAction="#buttonpressed"></Button>




</GridPane>